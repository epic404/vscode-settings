export AWS_REGION=us-west-2
export AWS_PROFILE=dev
export JAVA_HOME=$(/usr/libexec/java_home -v 17)

GPG_TTY=$(tty)
export GPG_TTY

startAirkit() {
	cd ~/repo/airkit/node/web-builder
	nvm use 14.17.5
	open https://localhost:8095
	npm start
}

goAirkit() {
	cd ~/repo/airkit/node/web-builder
}

buildAirkit() {
	cd ~/repo/airkit
	repo sync && service-tool --verbose && service-tool --sample-data-only -U bkent
}
